#include <stdio.h>
#include <stdlib.h>
#include <string.h>
/*
 * 
 */
int main(int argc, char** argv) {
    char sName[20];
    double numCP[20][2];

    
    FILE *inputFile = fopen("USCOVID19Data.txt","r");
    
    if(inputFile == NULL)
    {
        return(EXIT_FAILURE);
    }
    
    printf("\nCOVID-19 state data for November 14, 2020\n\n");
    printf("%2s%10s%15s%12s\n\n","ST","Cases","Population","Percent");
    
    int index = 0;
    while(!feof(inputFile)&&(index < 21))
    {
        fscanf(inputFile,"%s %lf %lf",sName,&numCP[index][0],&numCP[index][1]);
        numCP[index][2] = numCP[index][0]/numCP[index][1];
        printf("%2s%10.0f%15.0f%12.6f\n",sName,numCP[index][0],numCP[index][1],numCP[index][2]);
        index++;
    }
    
    printf("\nAll data has been read from USCOVID19Data.txt\n");
    

    return (EXIT_SUCCESS);
}

